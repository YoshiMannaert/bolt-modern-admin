{
    "name": "mosh/bolt-modern-admin",
    "description": "A modern admin theme for Bolt CMS",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoshi mannaert",
            "email": "yoshi.mannaert@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.9",
        "twig/twig": "^2.12 | ^3.0"
    },
    "require-dev": {
        "bolt/core": "^4.1",
        "symplify/easy-coding-standard": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Mosh\\BoltModernAdmin\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "Mosh\\BoltModernAdmin\\Extension"
    }
}
